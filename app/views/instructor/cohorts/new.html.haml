%h1 New Cohort

= form_for([:instructor, @cohort]) do |f|
  = errors_for(f)

  %p
    = f.label :name
    = f.text_field :name

  %p
    = f.label :start_date
    = f.text_field :start_date

  %p
    = f.label :end_date
    = f.text_field :end_date

  %p
    = f.label :google_maps_location
    = f.text_field :google_maps_location

  %p
    = f.label :directions
    = f.text_field :directions

  = f.submit "Save"

